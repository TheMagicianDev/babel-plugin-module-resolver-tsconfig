    "url": "git@github.com:TheMagicianDev/babel-plugin-module-resolver-tsconfig"
  },
  "keywords": [
    "babel-plugin-module-resolver",
    "helper",
    "tsconfig",
    "write",
    "aliases",
    "once",
    "typescript",
    "no-repetition"
  ],
  "author": "Mohamed Lamine Allal <allalmohamedlamine@gmail.com>",
  "license": "MIT",
  "types": "./index.d.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint lib/**/*.js test/**/*.test.js"
  },
